Create docker-compose.yml
version: '3.8'
services:
 jenkins:
 image: jenkins/jenkins:lts
 ports:
 - "8080:8080"
 - "50000:50000"
 volumes:
 - jenkins_home:/var/jenkins_home
volumes:
 jenkins_home:
docker compose up -d

docker exec jenkins cat /var/jenkins_home/secrets/initialAdminPassword(copy password)

